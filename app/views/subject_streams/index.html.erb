<div class="body-unit">
  <h1>Listing subject_streams</h1>
    <table>
    <thead>
      <tr>
        <th>Subject stream</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @subject_streams.each do |subject_stream| %>
        <tr>
          <td><%= subject_stream.subject_stream %></td>
          <td><%= link_to 'Show', subject_stream %></td>
          <td><%= link_to 'Edit', edit_subject_stream_path(subject_stream) %></td>
          <td><%= link_to 'Destroy', subject_stream, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to 'New Subject stream', new_subject_stream_path %>
</div>
