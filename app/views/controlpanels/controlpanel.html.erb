<div class="body-unit">
  <h1>Control Panel</h1>
  <p>
    <h4>Categories: <%= link_to "Settings", categories_path %><br>
    Streams: <%= link_to "Settings", streams_path %><br>
    Cutoff : <%= link_to "Settings", cutoffs_path %><br>
    Registration process: <%= link_to "Settings", registers_path %></h4>
    <% if Rank.exists? %>
      <%= link_to "Reset ranks", controlpanels_resetranks_path,
          data: {confirm: "Are you sure to reset ranks ?"} %>
    <% else %>
      <%= link_to "Generate ranks", controlpanels_generateranks_path,
          data: {confirm: "Are you sure to Generate ranks ?"} %>
    <% end %>
  </p>
</div>
